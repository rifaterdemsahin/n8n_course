    {
      "parameters": {
        "values": {
          "string": [
            {
              "name": "employeeId",
              "value": "={{Math.floor(Math.random() * 10000) + 1000}}"
            },
            {
              "name": "fullName",
              "value": "={{$json.firstName}} {{$json.lastName}}"
            },
            {
              "name": "email",
              "value": "={{$json.email}}"
            },
            {
              "name": "department",
              "value": "={{$json.department}}"
